div.box
    div.box-header
        h3.box-title
            a(href="" ui-sref="^.list({trailId: currentTrailId })" class="btn btn-default")
                i.fa.fa-arrow-left &nbsp;Back to Trail URLs
    div.box-body
        div
            h2 {{trailUrl.name}}

            h3 Associated Trail
                ul
                    li.label.label-default( ng-repeat="comment in trailUrl.trailId") {{trailId.id}}
            h3 Raw
                pre {{trailUrl|json}}