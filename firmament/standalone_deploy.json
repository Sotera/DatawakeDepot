[
  {
    "name": "data-container",
    "Image": "jreeme/data-container:7.0",
    "DockerFilePath": "docker/data-container",
    "Hostname": "data-container"
  },
  {
    "name": "mongo",
    "Image": "jreeme\/mongo:2.6",
    "DockerFilePath": "docker\/mongo\/2.6",
    "Hostname": "mongo",
    "HostConfig": {
      "VolumesFrom": [
        "data-container"
      ],
      "PortBindings": {
        "27017\/tcp": [
          {
            "HostPort": "27017"
          }
        ]
      }
    }
  },
  {
    "name": "datawake",
    "Image": "jreeme/strongloop:10",
    "DockerFilePath": "docker/strong-pm",
    "Hostname": "datawake",
    "ExposedPorts": {
      "3001/tcp": {}
    },
    "HostConfig": {
      "Links": [
        "mongo:mongo"
      ],
      "PortBindings": {
        "3001/tcp": [
          {
            "HostPort": "3000"
          }
        ],
        "8701/tcp": [
          {
            "HostPort": "8702"
          }
        ]
      }
    },
    "ExpressApps": [
      {
        "GitUrl": "https://github.com/Sotera/DatawakeDepot",
        "GitSrcBranchName": "develop",
        "StrongLoopBranchName": "deploy",
        "StrongLoopServerUrl": "http://localhost:8702",
        "ServiceName": "Datawake"
      }
    ]
  },
  {
    "name": "rancor",
    "Image": "jreeme/strongloop:12",
    "DockerFilePath": "docker/strong-pm",
    "Hostname": "rancor",
    "ExposedPorts": {
      "3001/tcp": {}
    },
    "HostConfig": {
      "Links": [
        "mongo:mongo"
      ],
      "PortBindings": {
        "3001/tcp": [
          {
            "HostPort": "3004"
          }
        ],
        "8701/tcp": [
          {
            "HostPort": "8701"
          }
        ]
      }
    },
    "ExpressApps": [
      {
        "GitUrl": "https://github.com/justinlueders/PageRancor.git",
        "GitSrcBranchName": "master",
        "StrongLoopBranchName": "deploy",
        "StrongLoopServerUrl": "http://localhost:8701",
        "ServiceName": "DatawakeRancor",
        "Scripts":[
          {
            "RelativeWorkingDir":".",
            "Command": "bower",
            "Args":[
              "install",
              "--config.interactive=false"
            ]
          }
        ]
      }
    ]
  },
  {
    "name": "stanNER",
    "Image": "jreeme/sljava",
    "DockerFilePath": "docker/strong-pm-java8",
    "Hostname": "stanNER",
    "ExposedPorts": {
      "3001/tcp": {}
    },
    "HostConfig": {
      "PortBindings": {
        "3001/tcp": [
          {
            "HostPort": "3003"
          }
        ],
        "8701/tcp": [
          {
            "HostPort": "8703"
          }
        ]
      }
    },
    "ExpressApps": [
      {
        "GitUrl": "https://github.com/Sotera/DatawakeSampleExtractor",
        "GitSrcBranchName": "stanford-ner",
        "StrongLoopBranchName": "deploy",
        "StrongLoopServerUrl": "http://localhost:8703",
        "ServiceName": "DatawakeStanfordNER"
      }
    ]
  }
]
